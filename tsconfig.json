{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "src",
    "App.tsx",
    "screens",
    "components",
    "navigation",
    "data",
    "types"
  ],
  "extends": "expo/tsconfig.base"
}
